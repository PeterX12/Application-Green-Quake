<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Application_Green_Quake.Views.EcoActions.EcoActionsSubMenu.HomePage"
             Title="Home Subcategories">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Air Out Your Home"
                    Clicked="NavigateToAirOutHome"/>
            <Button Text="Go Outside Once a Day"
                    Clicked="NavigateToOutsideOnce"/>
            <Button Text="Bring a Plant Into the Home"
                    Clicked="NavigateToPlantIntoHome"/>
            <Button Text="Clean With Non Harmful Products"
                    Clicked="NavigateToNonHarmfulProducts"/>
            <Button Text="Use Cloth Napkin Instead of Paper"
                    Clicked="NavigateToClothNapkins"/>
            <Button Text="Use Cloth Towels Instead of Paper"
                    Clicked="NavigateToClothTowels"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>