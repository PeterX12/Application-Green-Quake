<?xml version="1.0" encoding="utf-8" ?>    
    
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:reusable="clr-namespace:Application_Green_Quake.Reusable" xmlns:application_green_quake="clr-namespace:Application_Green_Quake"
            x:Class="Application_Green_Quake.MainMenu"
            NavigationPage.HasBackButton="False"
            NavigationPage.HasNavigationBar="False">

    <ContentPage Title="Main Menu">
        <StackLayout>
            <Button Text="Log Eco Action"
                    Clicked="NavigateToEcoActionButton" />
            <Button Text="Refill Station"
                    Clicked="NavigateToRefillStation" />
            <Button Text="Food Inverntory"
                    Clicked="NavigateToFoodInventory" />
            <Button Text="Sign Out"
                    Clicked="SignOutButton" />
        </StackLayout>
    </ContentPage>

    <reusable:TopTabLeaderBoard Title="Leaderboard"></reusable:TopTabLeaderBoard>
    <reusable:TopTabProfile Title="Profile"></reusable:TopTabProfile>
</TabbedPage>